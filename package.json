{
  "name": "sc-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "weird-start": "ts-node -r dotenv/config src/index.ts",
    "build": "tsc",
    "cf": "ts-node --files -r dotenv/config src/modules/utils/cf-sign.ts",
    "prep-db": "ts-node -r dotenv/config src/lib/util.create-database.ts",
    "drop-db": "ts-node -r dotenv/config src/lib/util.drop-dev-database.ts",
    "local-start": "tsc && node -r dotenv/config src/index.ts",
    "dev": "NODE_ENV=development nodemon -r dotenv/config --exec ts-node src/index.ts",
    "tsndev": "ts-node-dev --respawn src/index.ts",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run db:setup && TERM=dumb jest --detectOpenHandles",
    "tdev": "npm run db:setup && TERM=dumb jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "apollo-server-express": "2.9.9",
    "aws-sdk": "^2.812.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "colors": "^1.4.0",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dbmate": "^1.0.0",
    "dlv": "^1.1.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql": "^14.2.0",
    "graphql-query-complexity": "^0.2.3",
    "graphql-redis-subscriptions": "^2.1.2",
    "http": "^0.0.0",
    "install": "^0.13.0",
    "internal-ip": "^5.0.0",
    "ioredis": "^4.9.0",
    "nodemailer": "^6.0.0",
    "pg": "^7.9.0",
    "pgtools": "^0.3.0",
    "pino": "^5.14.0",
    "pino-pretty": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.1",
    "typeorm": "0.2.21",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.9",
    "@types/cors": "^2.8.4",
    "@types/dlv": "^1.1.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/graphql": "^14.2.0",
    "@types/ioredis": "^4.14.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "@types/nodemailer": "^4.6.7",
    "@types/pino": "^5.14.0",
    "@types/uuid": "^3.4.4",
    "casual": "^1.6.0",
    "chalk": "^2.4.2",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "3.7.5",
    "util": "^0.12.1"
  }
}
