{
  "name": "sc-backend",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "engines": {
    "node": "12.16.2"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "weird-start": "ts-node -r dotenv/config src/server.ts",
    "build": "tsc",
    "cf": "ts-node -T --files -r dotenv/config src/modules/utils/cf-sign.ts",
    "prep-db": "ts-node -r dotenv/config src/lib/util.create-database.ts",
    "drop-db": "ts-node -r dotenv/config src/lib/util.drop-dev-database.ts",
    "local-start": "tsc && node -r dotenv/config src/server.ts",
    "dev": "NODE_ENV=development nodemon -r dotenv/config --exec ts-node --type-check src/server.ts",
    "tsndev": "ts-node-dev --respawn src/server.ts",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run db:setup && TERM=dumb jest --detectOpenHandles",
    "tdev": "npm run db:setup && TERM=dumb jest --watch --detectOpenHandles",
    "upload-cert": "ts-node -r dotenv/config src/lib/util.upload-certificate.ts",
    "list-certs": "ts-node -r dotenv/config src/lib/util.list-certificates.ts"
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.3.0",
    "apollo-server-express": "2.21.0",
    "aws-sdk": "^2.812.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "colors": "^1.4.0",
    "connect-redis": "^3.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dbmate": "^1.0.0",
    "dlv": "^1.1.3",
    "dom-parser": "^0.1.6",
    "dot-prop": "4.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql": "15.3.0",
    "graphql-auth-user-directives": "^2.4.3",
    "graphql-query-complexity": "^0.2.3",
    "graphql-redis-subscriptions": "^2.1.2",
    "graphql-scalars": "^1.9.0",
    "graphql-subscriptions": "1.0.0",
    "http": "^0.0.0",
    "install": "^0.13.0",
    "internal-ip": "^5.0.0",
    "ioredis": "4.19.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "nodemailer": "^6.0.0",
    "objection": "^2.2.14",
    "objection-authorize": "^4.1.1",
    "pg": "^8.5.1",
    "pgtools": "^0.3.0",
    "pino": "^5.14.0",
    "pino-pretty": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.21",
    "uuid": "^3.3.2",
    "xmldom": "^0.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.9",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.4",
    "@types/dlv": "^1.1.2",
    "@types/dom-parser": "^0.1.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/graphql": "^14.2.0",
    "@types/ioredis": "^4.14.3",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^11.12.2",
    "@types/nodemailer": "^4.6.7",
    "@types/pino": "^5.14.0",
    "@types/uuid": "^3.4.4",
    "@types/xmldom": "^0.1.30",
    "casual": "^1.6.0",
    "chalk": "^2.4.2",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^4.1.3",
    "util": "^0.12.1"
  }
}
